{
	"name": "I4All",
	"description": "Making Web Accessible 4 All",
	"version": "1.0",
	"manifest_version": 2,
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},
	"permissions": ["activeTab", "clipboardRead", "storage"]
}
